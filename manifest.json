{
  "manifest_version" : 2,

  "name" : "Morse Code Translator",
  "description" : "This extension provides a morse code translation pop-out",
  "version" : "0.6",

  "icons" : {
    "16" : "img/icon16.png",
    "48" : "img/icon48.png",
    "128" : "img/icon128.png"
  },

  "browser_action" : {
    "default_icon" : "img/icon.png",
    "default_title" : "Translate Morse Code",
    "default_popup" : "html/index.html"
  }
}
